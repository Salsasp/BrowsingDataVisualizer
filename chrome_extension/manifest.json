{
    "manifest_version": 3,
    "name": "Browser Data Visualizer Tracker Companion",
    "version": "1.0",

    "action": {
        "default_popup": "popup.html"
      },

      "background": {
        "scripts": ["background.js"]
      },

      "host_permissions": [
        "https://developer.chrome.com/*"
      ],
    
      "permissions": [
        "history"
      ]
  }